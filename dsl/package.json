{
  "name": "ranking-dsl-workspace",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "gen": "tsx src/codegen.ts",
    "gen:check": "tsx src/codegen.ts --check",
    "build": "pnpm --filter './packages/*' run build",
    "typecheck": "pnpm -r run typecheck",
    "lint": "eslint src/**/*.ts *.js",
    "dslc": "node packages/compiler/dist/cli.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@iarna/toml": "^2.2.5",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  }
}
