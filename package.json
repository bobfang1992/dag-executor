{
  "name": "dag-executor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run gen && pnpm run build:dsl && pnpm run plan:build:all",
    "gen": "pnpm -C dsl run gen",
    "build:dsl": "pnpm -r run build",
    "dslc": "node dsl/packages/compiler/dist/cli.js",
    "plan:build:all": "tsx dsl/tools/build_all_plans.ts --backend quickjs",
    "plan:build:all:node": "tsx dsl/tools/build_all_plans.ts --backend node",
    "plan:build:node": "tsx dsl/packages/compiler-node/src/cli.ts"
  },
  "dependencies": {
    "@ranking-dsl/runtime": "workspace:*",
    "@ranking-dsl/generated": "workspace:*"
  },
  "devDependencies": {
    "tsx": "^4.6.0"
  }
}
